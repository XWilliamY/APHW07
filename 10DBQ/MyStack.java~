import java.util.*;

public class MyStack{

    private int [] stack = new int[10];
    private int head;
    private int tail;
    private int counter = 0;

    public MyStack(int size){
	stack = new int [size];
    }

    public MyStack(){
	stack = new int [10];
	}

    public int push(int value){
	stack[counter] = value; //add value
	counter ++; //increment counter
	return value; 
    }

    public int pop(){
	int removed = stack[counter];
	counter --; // moving back one treats the former as an empty space
	return removed;    
    }

    public int peek(){
	int value = stack[counter];
	return value;
    }

    public boolean empty(){
	return counter == 0;
    }

    public static void main(String[]args){
	MyStack A = new MyStack();
	System.out.println(A.push(5));
    }
}



